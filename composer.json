{
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.18",
        "phpunit/phpunit":"^9.0",
        "karneaud/swagger-php-generator": "1.0.0-alpha",
        "symfony/debug-bundle": "^6.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
              "name": "karneaud/swagger-php-generator",
              "version": "1.0.0-alpha",
              "type": "package",
              "require": {
                "php":">=7.4 || ^8.0",
                "nette/php-generator": "^3.0",
                "symfony/yaml": "^3.4",
                "symfony/serializer": "^3.4",
                "psr/http-message": "^1.0"
              },
              "autoload": {
                "psr-4": {
                  "SwaggerGen\\": "src/"
                }
              },
              "bin":[
                "bin/generate"
              ],
              "source": {
                "url": "https://github.com/karneaud/swagger-php-generator.git",
                "type": "git",
                "reference": "dev"
              }
            }
          }],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "require": {
        "twig/twig": "^3.0"
    },
    "scripts":{
        "generate":["generate"],
        "tests":["phpunit"]
    },
    "autoload": {
      "psr-4": {
        "Karneaud\\Generator\\":"src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Karneaud\\Generator\\Tests\\":"tests/"
      }
    }
}
